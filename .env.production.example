DB_USERNAME=root  # ⚠️ Ganti dengan username database hosting
DB_PASSWORD=  # ⚠️ Ganti dengan password database hosting

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

