# PRODUCTION ENVIRONMENT - Salin ke .env setelah deploy

APP_NAME="Perpustakaan Keliling Parepare"
APP_ENV=production
APP_KEY=base64:YOUR_APP_KEY_HERE
APP_DEBUG=false
APP_URL=https://yourdomain.com  # ⚠️ GANTI dengan domain Anda

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=error

DB_CONNECTION=mysql
DB_HOST=127.0.0.1  # Atau host database dari hosting
DB_PORT=3306
